#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/working/blogs/javafree-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/working/blogs/javafree-blog/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+vite@4.1.1_sass@1.58.2/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+vite@4.1.1_sass@1.58.2/node_modules/vite/bin/vite.js" "$@"
fi
